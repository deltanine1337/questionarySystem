<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>registerServlet</servlet-name>
        <servlet-class>servlets.Register</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>registerServlet</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>logInServlet</servlet-name>
        <servlet-class>servlets.LogIn</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>logInServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>questionariesServlet</servlet-name>
        <servlet-class>servlets.Questionaries</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>questionariesServlet</servlet-name>
        <url-pattern>/questionaries</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>questionsServlet</servlet-name>
        <servlet-class>servlets.Questions</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>questionsServlet</servlet-name>
        <url-pattern>/questions</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>editAnswersServlet</servlet-name>
        <servlet-class>servlets.EditAnswers</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>editAnswersServlet</servlet-name>
        <url-pattern>/editAnswers</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>fillQuestionaryServlet</servlet-name>
        <servlet-class>servlets.FillQuestionary</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>fillQuestionaryServlet</servlet-name>
        <url-pattern>/fillQuestionary</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>viewUserAnswersServlet</servlet-name>
        <servlet-class>servlets.ViewUserAnswers</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>viewUserAnswersServlet</servlet-name>
        <url-pattern>/viewUserAnswers</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>createDBServlet</servlet-name>
        <servlet-class>servlets.CreateDB</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>createDBServlet</servlet-name>
        <url-pattern>/createDB</url-pattern>
    </servlet-mapping>
</web-app>